void setup() {
 size(500,500); 
}


void draw() {
  
  pushMatrix();
  translate(width*0.5,height*0.5);
  ellipse(0,0, 20,20); 
  //translate(-width*0.5,-height*0.5);
  popMatrix();
  
  pushMatrix();
  translate(width*0.5 - 120*0.5,height*0.5 - 120*0.5);
  
  rect(0,0,20,20);
  rect(100,0,20,20);
  rect(0,100,20,20);
  rect(100,100,20,20);
  
  popMatrix();
 
  
}
